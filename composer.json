{
    "name": "binarybuilds/nova-advanced-command-runner",
    "description": "Laravel Nova tool for running Artisan and bash(shell) commands.",
    "keywords": ["laravel", "nova", "Artisan", "commands","shell","bash"],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "optimistdigital/nova-tailwind": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "BinaryBuilds\\NovaAdvancedCommandRunner\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BinaryBuilds\\NovaAdvancedCommandRunner\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
